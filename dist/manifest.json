{
    "name": "Tabs to notion",
    "description": "Save urls to notion",
    "version": "1.0",
    "manifest_version": 3,
    "action": {
        "default_popup": "popup.html"
    },
    "permissions": [
        "tabs",
        "bookmarks",
        "unlimitedStorage",
        "tabGroups"
      ],
    "author": "4dmu",
    "content_scripts": [{
        "matches": ["<all_urls>"],
        "js": ["contentscript.js"]
    }]
}